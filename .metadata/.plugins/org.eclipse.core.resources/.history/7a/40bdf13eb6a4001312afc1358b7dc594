package com.umons.exception;

/**
 * Exception call when the user try to use an existing location
 * 
 * @author AGOZZINO Terencio - PIZZIRUSSO Loris
 * 
 */

public class ArrayIndexOuOfBoundsExceptio extends Exception
{
	private static final long serialVersionUID = 1L;

	public ArrayIndexOuOfBoundsExceptio()
	{
		System.out.println("Please, select an other position. Thanks");
	}
}